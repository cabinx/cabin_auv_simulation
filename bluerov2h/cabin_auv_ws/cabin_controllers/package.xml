<?xml version="1.0"?>
<package format="2">
  <name>cabin_controllers</name>
  <version>0.0.1</version>
  <description>Controllers for cabin's test underwater vehicle</description>

  <maintainer email="xiekb@pcl.ac.cn">Cabin</maintainer>

  <license>BSD</license>


